:assertion {
    gda:association_000004 a sio:statistical-association;
        sio:has-measurement-value gda:association_000004_p_value;
        sio:refers-to <http://bio2rdf.org/geneid:4669>,
            <http://bio2rdf.org/omim:252900>;
        rdfs:comment "This is a statistical association between a gene (Entrenz gene id 4669) and a disease (OMIM 252900). It is generated by Concept Profile Matching method and has p-value of 0.0003." .

    gda:association_000004_p_value a sio:probability_value;
        sio:has-value "0.000262"^^xsd:float .

}

