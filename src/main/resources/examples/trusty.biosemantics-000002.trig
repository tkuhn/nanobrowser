sub:assertion {
	gda:association_000002 sio:has-measurement-value gda:association_000002_p_value ;
		sio:refers-to <http://bio2rdf.org/geneid:138050> , <http://bio2rdf.org/omim:252900> ;
		a sio:statistical-association ;
		rdfs:comment "This is a statistical association between a gene (Entrenz gene id 138050) and a disease (OMIM 252900). It is generated by Concept Profile Matching method and has p-value of 0.0003." .
	
	gda:association_000002_p_value sio:has-value "0.000262"^^xsd:float ;
		a sio:probability_value .
}

