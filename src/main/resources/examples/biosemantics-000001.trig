@base <http://rdf.biosemantics.org/nanopubs/gene_disease_associations/000001> .

:head {
    <> a np:Nanopublication;
        np:hasAssertion :assertion;
        np:hasProvenance :provenance .

    :provenance a np:Provenance;
        np:hasAttribution :Attribution ;
        np:hasSupporting :Supporting .

}

:assertion {
    gda:association_000001 a sio:statistical-association;
        sio:has-measurement-value gda:association_000001_p_value;
        sio:refers-to <http://bio2rdf.org/geneid:6448>,
            <http://bio2rdf.org/omim:252900>;
        rdfs:comment "This is a statistical association between a gene (Entrenz gene id 6448) and a disease (OMIM 252900). It is generated by Concept Profile Matching method and has p-value of 0.0001." .

    gda:association_000001_p_value a sio:probability_value;
        sio:has-value "6.56e-05"^^xsd:float .

}

:Attribution {
    <>  dcterms:created "2012-06-08T14:15:31.263073"^^xsd:dateTime;
        dcterms:isPartOf <http://rdf.biosemantics.org/nanopubs/gene_disease_associations>;
        dcterms:rightsHolder <http://biosemantics.org>;
        pav:authoredBy <http://www.researcherid.com/rid/B-5927-2012>,
            <http://www.researcherid.com/rid/B-6035-2012>;
        pav:createdBy <http://www.researcherid.com/rid/B-5852-2012>;
        pav:versionNumber "1.0".
    :assertion  dcterms:created "2012-02-09"^^xsd:date;
                pav:authoredBy <http://www.researcherid.com/rid/B-5927-2012>,
                    <http://www.researcherid.com/rid/B-6035-2012> .

}

:Supporting {
    <>  dcterms:isPartOf <http://rdf.biosemantics.org/nanopubs/gene_disease_associations> .
    :assertion  opm:wasDerivedFrom <http://rdf.biosemantics.org/vocabularies/text_mining/gene_disease_concept_profiles_1980_2010>;
                opm:wasGeneratedBy <http://rdf.biosemantics.org/vocabularies/text_mining/gene_disease_concept_profiles_matching_1980_2010> .

}
